{
    "name": "tobento/app-file-storage",
    "type": "library",
    "description": "App file storage support.",
    "keywords": [
        "tobento",
        "package",
        "app",
        "file",
        "storage"
    ],
    "homepage": "https://www.tobento.ch",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Strub",
            "email": "info@tobento.ch",
            "homepage": "https://www.tobento.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "tobento/app": "^1.0",
        "tobento/app-migration": "^1.0",
        "tobento/app-http": "^1.0.6",
        "tobento/service-file-storage": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.0",
        "tobento/service-filesystem": "^1.0",
        "nyholm/psr7": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Tobento\\App\\FileStorage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tobento\\App\\FileStorage\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always",
        "psalm": "vendor/bin/psalm"
    }
}